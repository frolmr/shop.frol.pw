div.content
    div.logo.logo_small
        img(src='images/logo.png').logo__image_small
    div.links
        div.stores
            a(href='#').apple
                img(src='images/apple.png').apple__image
            a(href='#').google
                img(src='images/google.png').google__image
        - var socials = [{name:'VK', link:'http://vk.com'},{name:'FB', link:'http://facebook.com'},{name:'TW', link:'http://twitter.com'}]
        ul.social
            each social in socials
                li.social__item
                    a(href=social.link target='blank').social__link.social__link_big(class='social__link_' + social.name)=social.name
